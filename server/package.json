{
  "name": "foodweb-server",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/server/projects/web/src/index.js",
  "scripts": {
    "build": "node ./tools/npm/build.js",
    "clean": "rimraf ./dist/* ./coverage ./ormlogs.log",
    "clean:all": "npm run clean && npm run clean:dump && rimraf ./node_modules",
    "clean:dump": "rimraf ./tools/database/dump/*",
    "init:install": "node ./tools/npm/init-install.js",
    "init:project": "node ./tools/npm/init-project.js",
    "install:shared": "cd ../shared && npm install && cd ../server",
    "job": "node ./tools/npm/job.js",
    "lambda:pkg": "./tools/npm/lambda-pkg.sh",
    "lint": "node ./tools/npm/lint.js",
    "pg": "node ./tools/npm/pg.js",
    "postinstall": "npm run install:shared",
    "start": "npm run init:install && node ./tools/npm/start.js",
    "start:docker": "npm run start:prep && tsc-watch -p ./projects/web/tsconfig.json --noClear --onFirstSuccess \"nodemon -e .js,.hbs,.css --watch ./dist/server/projects/web/src/ --watch ./dist/shared/src/ --watch ./templates/ --inspect=0.0.0.0:9229 ./dist/server/projects/web/src/index.js\"",
    "templates:precompile": "node ./tools/templates/precompile.js",
    "test": "npm run lint && ts-mocha -p ./projects/web/tsconfig.spec.json ./src/**/*.ts",
    "test:watch": "ts-mocha -p ./projects/web/tsconfig.spec.json ./src/**/*.ts",
    "typeorm": "node ./tools/npm/typeorm.js"
  },
  "private": true,
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "compression": "^1.7.4",
    "connect-redis": "^5.1.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-query-boolean": "^2.0.0",
    "express-query-int": "^3.0.0",
    "express-session": "^1.17.1",
    "fs-extra": "^9.1.0",
    "geo-tz": "^6.0.1",
    "google-polyline": "^1.0.3",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.10.0",
    "is-port-reachable": "^3.0.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.2",
    "node-gcm": "^1.0.4",
    "node-geocoder": "^3.27.0",
    "nodemailer": "^6.5.0",
    "nodemailer-mailgun-transport": "^1.0.1",
    "pg": "^8.6.0",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.16",
    "@types/chai-http": "^3.0.5",
    "@types/connect-redis": "0.0.16",
    "@types/cors": "^2.8.10",
    "@types/cron": "^1.7.2",
    "@types/expect": "^1.20.4",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.2",
    "@types/moment-timezone": "^0.5.13",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.41",
    "@types/node-gcm": "^1.0.0",
    "@types/node-geocoder": "^3.24.1",
    "@types/node-schedule": "^1.3.1",
    "@types/nodemailer": "^6.4.1",
    "@types/pg": "^7.14.11",
    "@types/redis": "^2.8.28",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "command-exists": "^1.2.9",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "juice": "^7.0.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "prompts": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-essentials": "^7.0.1",
    "ts-loader": "^9.1.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4",
    "yargs": "^16.2.0"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "7.8.0"
  }
}
