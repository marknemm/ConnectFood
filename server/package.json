{
  "name": "foodweb-server",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/server/projects/web/src/index.js",
  "scripts": {
    "admin:build": "rimraf ./dist/* && tsc -p ./projects/admin/tsconfig.json && npm run templates:precompile",
    "admin:build:watch": "rimraf ./dist/* && tsc-watch -p ./projects/admin/tsconfig.json",
    "admin:start": "npm run admin:start:prep && tsc-watch -p ./projects/admin/tsconfig.localdev.json --noClear --onFirstSuccess \"nodemon -e .js,.hbs,.css --watch ./admin-dist/server/projects/web/src/ --watch ./admin-dist/server/projects/admin/src/ --watch ./admin-dist/server/database/src/ --watch ./admin-dist/shared/src/ --watch ./templates/ --watch ./projects/admin/.env --inspect=127.0.0.1:9230 ./admin-dist/server/projects/admin/src/index.js\"",
    "admin:start:docker": "npm run admin:start:prep && tsc-watch -p ./projects/admin/tsconfig.localdev.json --noClear --onFirstSuccess \"nodemon -e .js,.hbs,.css --watch ./admin-dist/server/projects/web/src/ --watch ./admin-dist/server/projects/admin/src/ --watch ./admin-dist/server/database/src/ --watch ./admin-dist/shared/src/ --watch ./templates/ --inspect=0.0.0.0:9230 ./admin-dist/server/projects/admin/src/index.js\"",
    "admin:start:prep": "node ./build-util/initial-install.js admin && rimraf ./admin-dist/* ",
    "build": "rimraf ./dist/* && tsc -p ./projects/web/tsconfig.json && npm run templates:precompile",
    "build:watch": "rimraf ./dist/* && tsc-watch -p ./projects/web/tsconfig.json",
    "clean": "rimraf ./dist/* ./coverage ./ormlogs.log",
    "clean:all": "npm run clean && npm run clean:dump && rimraf ./node_modules",
    "clean:dump": "rimraf ./database-util/dump/*",
    "install:all": "npm run install:shared && npm install",
    "install:shared": "cd ../shared && npm install && cd ../server",
    "job:auto-assign-donations": "node ./dist/server/jobs/src/auto-assign-donations.js",
    "job:delete-password-reset": "node ./dist/server/jobs/src/delete-password-reset.js",
    "job:send-delivery-reminders": "node ./dist/server/jobs/src/delete-password-reset.js",
    "lint": "tslint -p ./projects/admin/tsconfig.json && tslint -p ./projects/web/tsconfig.json",
    "pg:destroy": "node ./database-util/js/pg-exec.js -a -f ./database-util/sql/destroy-db.pgsql",
    "pg:dump": "node ./database-util/js/pg-dump.js",
    "pg:initialize": "npm run pg:destroy && node ./database-util/js/pg-initialize.js",
    "pg:make-timestamps-relevant": "node ./database-util/js/pg-exec.js -f ./database-util/sql/make-timestamps-relevant.pgsql",
    "pg:restore": "npm run pg:destroy && node ./database-util/js/pg-restore.js",
    "start": "npm run start:prep && tsc-watch -p ./projects/web/tsconfig.json --noClear --onFirstSuccess \"nodemon -e .js,.hbs,.css --watch ./dist/server/projects/web/src/ --watch ./dist/shared/src/ --watch ./templates/ --watch ./projects/web/.env --inspect ./dist/server/projects/web/src/index.js\"",
    "start:docker": "npm run start:prep && tsc-watch -p ./projects/web/tsconfig.json --noClear --onFirstSuccess \"nodemon -e .js,.hbs,.css --watch ./dist/server/projects/web/src/ --watch ./dist/shared/src/ --watch ./templates/ --inspect=0.0.0.0:9229 ./dist/server/projects/web/src/index.js\"",
    "start:prep": "node ./build-util/initial-install.js web && rimraf ./dist/*",
    "templates:precompile": "node ./templates/precompile-templates.js",
    "test": "npm run lint && ts-mocha -p ./projects/web/tsconfig.spec.json ./src/**/*.ts",
    "test:watch": "ts-mocha -p ./projects/web/tsconfig.spec.json ./src/**/*.ts",
    "typeorm:docker:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f docker-ormconfig.json",
    "typeorm:cli": "node ./database-util/js/typeorm-exec.js",
    "typeorm:migration:create": "npm run start:prep && npm run pg:dump && npm run typeorm:cli -- migration:create -n",
    "typeorm:migration:generate": "npm run start:prep && npm run pg:dump && npm run build && npm run typeorm:cli -- migration:generate -n",
    "typeorm:migration:revert": "npm run start:prep && npm run pg:dump && npm run typeorm:cli -- migration:revert",
    "typeorm:migration:run": "npm run start:prep && npm run pg:dump && npm run typeorm:cli -- migration:run"
  },
  "private": true,
  "dependencies": {
    "@google/maps": "^1.1.0",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/node": "^13.9.5",
    "await-to-js": "^2.1.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "compression": "^1.7.4",
    "connect-redis": "^3.4.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-force-https": "^1.0.0",
    "express-recaptcha": "^5.0.1",
    "express-session": "^1.17.0",
    "fs-extra": "^8.1.0",
    "geo-tz": "^5.1.0",
    "google-polyline": "^1.0.3",
    "handlebars": "^4.7.6",
    "handlebars-helpers": "^0.10.0",
    "juice": "^5.2.0",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "node-gcm": "^1.0.2",
    "node-geocoder": "^3.26.0",
    "nodemailer": "^6.4.6",
    "nodemailer-mailgun-transport": "^1.4.0",
    "pg": "^7.18.2",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "ts-essentials": "^6.0.4",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.25",
    "typescript": "^3.8.3",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/chai-http": "^3.0.5",
    "@types/connect-redis": "0.0.8",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/expect": "^1.20.4",
    "@types/fs-extra": "^7.0.0",
    "@types/google__maps": "^0.5.12",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/moment-timezone": "^0.5.13",
    "@types/multer": "^1.4.2",
    "@types/node-gcm": "^1.0.0",
    "@types/node-geocoder": "^3.24.1",
    "@types/node-schedule": "^1.3.0",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^7.14.3",
    "@types/redis": "^2.8.17",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "copyfiles": "^2.2.0",
    "is-port-reachable": "^3.0.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.4",
    "opn-cli": "^4.1.0",
    "rimraf": "^2.7.1",
    "run-sequence": "^2.0.0",
    "ts-mocha": "^2.0.0",
    "ts-node": "^8.8.1",
    "tsc-watch": "^2.4.0",
    "tslint": "^5.20.1",
    "yargs": "^15.3.1"
  },
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  }
}
