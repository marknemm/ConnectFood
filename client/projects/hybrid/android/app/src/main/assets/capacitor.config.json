{
	"appId": "io.foodweb.hybrid",
	"appName": "foodweb",
	"bundledWebRuntime": false,
	"npmClient": "npm",
	"webDir": "../../dist/hybrid",
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"SplashScreen": {
			"launchAutoHide": false,
			"launchShowDuration": 3000
		}
	},
	"android": {
		"allowMixedContent": true
	},
	"ios": {
		"contentInset": "automatic"
	},
	"cordova": {},
	"server": {}
}
