{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "admin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "prefix": "foodweb",
          "style": "scss"
        },
        "@schematics/angular:directive": {
          "prefix": "foodweb"
        }
      },
      "root": "projects/admin",
      "sourceRoot": "projects/admin/src",
      "prefix": "foodweb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/admin",
            "index": "projects/admin/src/index.html",
            "main": "projects/admin/src/main.ts",
            "polyfills": "projects/admin/src/polyfills.ts",
            "tsConfig": "projects/admin/tsconfig.json",
            "allowedCommonJsDependencies": [
              "event-source-polyfill",
              "google-polyline",
              "lscache"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "./assets",
                "output": "./assets"
              },
              "projects/admin/src/assets"
            ],
            "styles": [
              "styles/styles.scss",
              "projects/admin/src/styles/admin-styles.scss",
              "./node_modules/quill/dist/quill.core.css",
              "./node_modules/quill/dist/quill.bubble.css",
              "./node_modules/quill/dist/quill.snow.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "styles",
                "projects/admin/src/styles"
              ]
            },
            "fileReplacements": [
              {
                "replace": "src/environments/environment.ts",
                "with": "projects/admin/src/environments/environment.ts"
              }
            ]
          },
          "configurations": {
            "hmr": {
              "fileReplacements": [
                {
                  "replace": "projects/admin/src/environments/environment.ts",
                  "with": "projects/admin/src/environments/environment.hmr.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "projects/admin/src/environments/environment.hmr.ts"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/admin/src/environments/environment.ts",
                  "with": "projects/admin/src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "projects/admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "admin:build",
            "sourceMap": true
          },
          "configurations": {
            "hmr": {
              "hmr": true,
              "hmrWarning": false,
              "browserTarget": "admin:build:hmr"
            },
            "production": {
              "browserTarget": "admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/admin/src/test.ts",
            "polyfills": "projects/admin/src/polyfills.ts",
            "tsConfig": "projects/admin/tsconfig.spec.json",
            "karmaConfig": "projects/admin/karma.conf.js",
            "assets": [
              "assets"
            ],
            "styles": [
              "styles/styles.scss",
              "projects/admin/src/styles/admin-styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "styles",
                "projects/admin/src/styles"
              ]
            },
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/admin/tsconfig.json",
              "projects/admin/tsconfig.spec.json",
              "projects/admin/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/admin/e2e/protractor.conf.js",
            "devServerTarget": "admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "admin:serve:production"
            }
          }
        }
      }
    },
    "app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "prefix": "foodweb",
          "style": "scss"
        },
        "@schematics/angular:directive": {
          "prefix": "foodweb"
        }
      },
      "root": "projects/app",
      "sourceRoot": "projects/app/src",
      "cli": {
        "defaultCollection": "@nativescript/schematics"
      }
    },
    "web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "prefix": "foodweb",
          "style": "scss"
        },
        "@schematics/angular:directive": {
          "prefix": "foodweb"
        }
      },
      "root": ".",
      "sourceRoot": "projects/web/src",
      "prefix": "foodweb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/main",
            "index": "projects/web/src/index.html",
            "main": "projects/web/src/main.ts",
            "polyfills": "projects/web/src/polyfills.ts",
            "tsConfig": "projects/web/tsconfig.app.json",
            "allowedCommonJsDependencies": [
              "event-source-polyfill",
              "google-polyline",
              "lscache"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "./assets",
                "output": "./assets"
              },
              "projects/web/src/assets"
            ],
            "styles": [
              "styles/styles.scss",
              "projects/web/src/styles/main-styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "styles",
                "projects/web/src/styles"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "demo": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/web/src/environments/environment.ts",
                  "with": "projects/web/src/environments/environment.demo.ts"
                }
              ]
            },
            "demo-no-recaptcha": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/web/src/environments/environment.ts",
                  "with": "projects/web/src/environments/environment.demo-no-recaptcha.ts"
                }
              ]
            },
            "es5": {
              "tsConfig": "tsconfig.es5.json"
            },
            "hmr": {
              "fileReplacements": [
                {
                  "replace": "projects/web/src/environments/environment.ts",
                  "with": "projects/web/src/environments/environment.hmr.ts"
                }
              ]
            },
            "offline": {
              "fileReplacements": [
                {
                  "replace": "projects/web/src/environments/environment.ts",
                  "with": "projects/web/src/environments/environment.offline.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/web/src/environments/environment.ts",
                  "with": "projects/web/src/environments/environment.prod.ts"
                }
              ]
            },
            "production-no-recaptcha": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/web/src/environments/environment.ts",
                  "with": "projects/web/src/environments/environment.prod-no-recaptcha.ts"
                }
              ]
            },
            "qa": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/web/src/environments/environment.ts",
                  "with": "projects/web/src/environments/environment.qa.ts"
                }
              ]
            },
            "qa-no-recaptcha": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/web/src/environments/environment.ts",
                  "with": "projects/web/src/environments/environment.qa-no-recaptcha.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web:build",
            "sourceMap": true
          },
          "configurations": {
            "es5": {
              "browserTarget": "web:build:es5"
            },
            "hmr": {
              "hmr": true,
              "hmrWarning": false,
              "browserTarget": "web:build:hmr"
            },
            "production": {
              "browserTarget": "web:build:production"
            },
            "qa": {
              "browserTarget": "web:build:qa"
            },
            "offline": {
              "browserTarget": "web:build:offline"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "styles/styles.scss",
              "src/styles/main-styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "styles",
                "src/styles"
              ]
            },
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "web:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "defaultProject": "web"
}
