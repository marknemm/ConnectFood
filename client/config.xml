<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wnyfoodweb.foodweb" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FoodWeb</name>
    <description>
        Our mission is to help alleviate hunger and prevent the waste of food.
        With our proprietary platform and our network of volunteers, we will enable local businesses to easily and quickly donate their excess food to local charities.
    </description>
    <author email="foodweb.manager@gmail.com" href="https://www.wnyfoodweb.com">
        FoodWeb Team
    </author>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="fb:*" />
    <allow-intent href="twitter:*" />
    <allow-intent href="youtube:*" />
    <allow-intent href="linkedin:*" />
    <allow-intent href="pinterest:*" />
    <allow-intent href="instagram:*" />
    <allow-intent href="snapchat:*" />
    <icon src="res/icon.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application">
            <application android:usesCleartextTraffic="true" />
        </edit-config>
        <preference name="CodePushDeploymentKey" value="YsZZBYU-bVuHZZOapXW_qYeuzA_lTgBE2ND2K" />
        <resource-file src="google-services.json" target="app/google-services.json" />
        <resource-file src="res/android/icon/notification/icon-mdpi.png" target="app/src/main/res/drawable-mdpi/notification.png" />
        <resource-file src="res/android/icon/notification/icon-hdpi.png" target="app/src/main/res/drawable-hdpi/notification.png" />
