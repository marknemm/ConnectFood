        <resource-file src="res/android/icon/notification/icon-xxxhdpi.png" target="app/src/main/res/drawable-xxxhdpi/notification.png" />
        <splash density="mdpi" src="res/android/splash/drawable-mdpi/splash.9.png" />
        <splash density="hdpi" src="res/android/splash/drawable-hdpi/splash.9.png" />
        <splash density="xhdpi" src="res/android/splash/drawable-xhdpi/splash.9.png" />
        <splash density="xxhdpi" src="res/android/splash/drawable-xxhdpi/splash.9.png" />
