{
  "name": "foodweb",
  "displayName": "FoodWeb",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "angular-cli": {},
  "main": "client/dist/index.html",
  "scripts": {
    "build": "ng build --prod --aot",
    "build:all": "npm run build && npm run build:mobile",
    "build:e2e": "ng build --watch --configuration=e2e",
    "build:env": "ng build --prod --aot --configuration=${environment}",
    "build:mobile": "ng build --configuration=mobile --baseHref=./ --prod --aot && cd ./mobile && npm run build",
    "build:mobile:watch": "ng build --configuration=mobile-debug --baseHref=./ --watch",
    "build:mobile:debug": "ng build --configuration=mobile-debug --baseHref=./ && npm run deploy:mobile:debug",
    "build:watch": "ng build --watch",
    "clean": "rimraf ./dist ./coverage",
    "clean:all": "npm run clean && rimraf ./node_modules && npm run clean:mobile",
    "clean:mobile": "rimraf ./mobile/www/*",
    "deploy:mobile:debug": "cd ./mobile && npm run build:debug && cd ..",
    "hmr": "ng serve --configuration hmr",
    "lint": "ng lint",
    "start:offline": "ng serve --proxy-config proxy.conf.json --configuration=offline",
    "postinstall": "cd ./mobile && npm install && cd ..",
    "start": "ng serve --proxy-config proxy.conf.json",
    "test": "npm run lint && ng test --watch=false --code-coverage --source-map",
    "test:watch": "ng test --code-coverage --source-map"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0",
    "@angular-devkit/build-angular": "~0.801.0",
    "@angular/animations": "^8.1.0",
    "@angular/cdk": "^8.1.4",
    "@angular/cli": "^8.1.0",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/language-service": "^8.1.0",
    "@angular/material": "^8.1.4",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@angularclass/hmr": "^2.1.3",
    "bootstrap-css-only": "^4.2.1",
    "event-source-polyfill": "^1.0.8",
    "eventsource": "^1.0.7",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "keycode": "^2.2.0",
    "lodash": "^4.17.15",
    "ng-recaptcha": "^5.0.0",
    "ngx-ellipsis": "^2.0.5",
    "ngx-gallery": "^5.10.0",
    "ngx-img-cropper": "^8.0.1",
    "ngx-mask": "^8.0.1",
    "ngx-material-timepicker": "^5.0.0",
    "rxjs": "^6.5.3",
    "text-mask-addons": "^3.8.0",
    "typescript": "~3.4.5",
    "utility-types": "^3.7.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@types/cordova": "0.0.34",
    "@types/eventsource": "^1.1.2",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "^3.3.12",
    "@types/lodash": "^4.14.118",
    "@types/moment-timezone": "^0.5.9",
    "@types/node": "^10.12.12",
    "codelyzer": "^5.1.0",
    "concurrently": "^4.1.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-spec-reporter": "0.0.32",
    "protractor": "^5.4.1",
    "symlink-dir": "^3.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0"
  }
}
