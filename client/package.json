{
  "name": "foodweb",
  "displayName": "FoodWeb",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "angular-cli": {},
  "main": "client/dist/index.html",
  "scripts": {
    "build": "ng build --prod --aot",
    "build:all": "npm run build && npm run mobile:build:release",
    "build:e2e": "ng build --watch --configuration=e2e",
    "build:env": "ng build --prod --aot --configuration=${environment}",
    "build:watch": "ng build --watch",
    "clean": "rimraf ./dist ./coverage",
    "clean:all": "npm run clean && rimraf ./node_modules && npm run clean:mobile",
    "hmr": "ng serve --configuration hmr",
    "lint": "ng lint",
    "mobile:build": "npm run mobile:build:angular && npm run mobile:deploy",
    "mobile:build:angular": "ng build --configuration=mobile-debug --baseHref=./",
    "mobile:build:angular:watch": "ng build --watch --configuration=mobile-debug --baseHref=./ --output-path=./mobile/www",
    "mobile:build:release": "ng build --configuration=mobile --baseHref=./ --prod --aot && cd ./mobile && npm run build && cd ../",
    "mobile:build:start:android": "npm run mobile:build && npm run mobile:start:android",
    "mobile:build:start:browser": "npm run mobile:build && npm run mobile:start:browser",
    "mobile:build:start:ios": "npm run mobile:build && npm run mobile:start:ios",
    "mobile:build:watch": "concurrently \"npm run mobile:build:angular:watch\" \"npm run mobile:prepare:watch\"",
    "mobile:clean": "rimraf ./mobile/www/* && cd ./mobile && npm run clean && cd ../",
    "mobile:clean:all": "cd ./mobile && npm run clean:all && cd ../",
    "mobile:deploy": "cd ./mobile && npm run build:debug && cd ../",
    "mobile:clean:start:android": "npm run mobile:clean:all && npm run mobile:build:start:android",
    "mobile:clean:start:browser": "npm run mobile:clean:all && npm run mobile:build:start:browser",
    "mobile:clean:start:ios": "npm run mobile:clean:all && npm run mobile:build:start:ios",
    "mobile:prepare": "cd ./mobile && npm run prepare && cd ../",
    "mobile:prepare:watch": "watch \"npm run mobile:prepare\" ./mobile/www --interval=5",
    "mobile:prepare:build": "npm run mobile:prepare && npm run mobile:build",
    "mobile:start:android": "cd ./mobile && npm run start:android && cd ../",
    "mobile:start:browser": "cd ./mobile && npm run start:browser && cd ../",
    "mobile:start:ios": "cd ./mobile && npm run start:ios && cd ../",
    "start:offline": "ng serve --proxy-config proxy.conf.json --configuration=offline",
    "postinstall": "cd ./mobile && npm install && cd ..",
    "start": "ng serve --proxy-config proxy.conf.json",
    "test": "npm run lint && ng test --watch=false --code-coverage --source-map",
    "test:watch": "ng test --code-coverage --source-map"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0",
    "@angular-devkit/build-angular": "~0.801.0",
    "@angular/animations": "^8.1.0",
    "@angular/cdk": "^8.1.4",
    "@angular/cli": "^8.1.0",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/language-service": "^8.1.0",
    "@angular/material": "^8.1.4",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@angularclass/hmr": "^2.1.3",
    "bootstrap-css-only": "^4.2.1",
    "cordova": "^9.0.0",
    "cordova-plugin-device": "^2.0.3",
    "event-source-polyfill": "^1.0.8",
    "eventsource": "^1.0.7",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "keycode": "^2.2.0",
    "lodash": "^4.17.15",
    "ng-recaptcha": "^5.0.0",
    "ngx-ellipsis": "^2.0.5",
    "ngx-gallery": "^5.10.0",
    "ngx-img-cropper": "^8.0.1",
    "ngx-mask": "^8.0.1",
    "ngx-material-timepicker": "^5.0.0",
    "phonegap-plugin-push": "^2.3.0",
    "rxjs": "^6.5.3",
    "text-mask-addons": "^3.8.0",
    "typescript": "~3.4.5",
    "utility-types": "^3.7.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@types/cordova": "0.0.34",
    "@types/eventsource": "^1.1.2",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "^3.3.12",
    "@types/lodash": "^4.14.118",
    "@types/moment-timezone": "^0.5.9",
    "@types/node": "^10.12.12",
    "codelyzer": "^5.1.0",
    "concurrently": "^4.1.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-spec-reporter": "0.0.32",
    "protractor": "^5.4.1",
    "symlink-dir": "^3.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "watch": "^1.0.2"
  }
}
