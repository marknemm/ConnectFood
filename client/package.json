    "@angular/compiler-cli": "^7.2.8",
    "@angular/core": "^7.2.1",
    "@angular/forms": "^7.2.1",
    "@angular/http": "^7.2.1",
    "@angular/language-service": "^7.2.1",
    "@angular/material": "^7.2.1",
