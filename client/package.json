  "name": "food-web-client",
  "version": "1.0.0",
  "license": "MIT",
  "angular-cli": {},
  "main": "client/dist/index.html",
  "scripts": {
    "build": "ng build --prod --aot --configuration=production",
    "build:watch": "ng build --watch",
    "build:e2e": "ng build --watch --configuration=e2e",
    "clean": "rimraf ./dist ./coverage",
    "clean:all": "npm run clean && rimraf ./node_modules",
    "hmr": "ng serve --configuration hmr",
    "lint": "ng lint",
    "start": "npm run hmr",
    "test": "npm run lint && ng test --watch=false --code-coverage --source-map",
    "test:watch": "ng test --code-coverage --source-maps"
  },
  "private": true,
