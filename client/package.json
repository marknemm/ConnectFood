{
  "name": "foodweb",
  "displayName": "FoodWeb",
  "version": "3.0.0",
  "license": "Apache-2.0",
  "angular-cli": {},
  "main": "main.js",
  "scripts": {
    "all:build": "npm run web:build && npm run admin:build",
    "all:lint": "npm run web:lint && npm run admin:lint",
    "all:test": "npm run web:test && npm run admin:test",
    "admin:build": "npm run build:prep && ng build --project admin --prod --aot",
    "admin:build:env": "npm run build:prep && ng build --project admin --prod --aot --configuration=${environment}",
    "admin:build:watch": "npm run build:prep && ng build --project admin --watch",
    "admin:lint": "ng lint --project admin",
    "admin:start": "npm run build:prep && ng serve --project admin --port=4201",
    "admin:start:docker": "npm run build:prep && node ./build-util/initial-install.js && ng serve --project admin --port=4201 --host 0.0.0.0 --disableHostCheck=true",
    "admin:test": "npm run build:prep && npm run admin:lint && ng test --project admin --watch=false --code-coverage",
    "admin:test:watch": "npm run build:prep && ng test --project admin --code-coverage",
    "build": "npm run build:prep && ng build --prod --aot",
    "build:env": "npm run build:prep && ng build --prod --aot --configuration=${environment}",
    "build:prep": "node ./build-util/initial-install.js",
    "build:watch": "npm run build:prep && ng build --watch",
    "clean": "rimraf ./dist ./www ./platforms ./plugins ./coverage",
    "clean:all": "npm run clean && rimraf ./node_modules",
    "install:shared": "cd ../shared && npm install && cd ../client",
    "lint": "ng lint",
    "ng": "ng",
    "ngcc": "ngcc --properties es2015 module main --first-only",
    "postinstall": "npm run ngcc && npm run install:shared",
    "preview": "tns preview",
    "start": "npm run build:prep && ng serve",
    "start:docker": "npm run build:prep && node ./build-util/initial-install.js && ng serve --host 0.0.0.0 --disableHostCheck=true",
    "start:es5": "npm run build:prep && ng serve --configuration es5",
    "start:offline": "ng serve --configuration=offline",
    "test": "npm run build:prep && npm run web:lint && ng test --watch=false --code-coverage",
    "test:watch": "npm run build:prep && ng test --code-coverage",
    "watch": "npm-watch",
    "web:lint": "ng lint --project web"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marknemm/FoodWeb.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.10",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "^11.2.10",
    "@angular/compiler": "^11.2.10",
    "@angular/core": "^11.2.10",
    "@angular/forms": "^11.2.10",
    "@angular/google-maps": "^11.2.9",
    "@angular/language-service": "^11.2.10",
    "@angular/material": "^11.2.9",
    "@angular/platform-browser": "^11.2.10",
    "@angular/platform-browser-dynamic": "^11.2.10",
    "@angular/router": "^11.2.10",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "bootstrap-css-only": "^4.4.1",
    "core-js": "^3.10.1",
    "event-source-polyfill": "^1.0.22",
    "eventsource": "^1.1.0",
    "font-awesome": "^4.7.0",
    "lodash-es": "^4.17.21",
    "lscache": "^1.3.0",
    "material-design-icons": "^3.0.1",
    "ngx-material-timepicker": "^5.5.3",
    "ngx-quill": "^13.2.0",
    "quill": "^1.3.7",
    "reflect-metadata": "~0.1.13",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.9",
    "@angular/cli": "^11.2.9",
    "@angular/compiler-cli": "^11.2.10",
    "@types/eventsource": "^1.1.5",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "~3.6.9",
    "@types/lodash-es": "^4.17.4",
    "@types/lscache": "^1.3.0",
    "@types/node": "^14.14.41",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-spec-reporter": "0.0.32",
    "native-run": "^1.3.0",
    "ng-mocks": "^11.10.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.9.0",
    "protractor": "^7.0.0",
    "symlink-dir": "^5.0.0",
    "ts-essentials": "^7.0.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "~4.1.0",
    "watch": "^1.0.2",
    "webpack": "^4.46.0",
    "yargs": "^16.2.0"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "7.8.0"
  }
}
